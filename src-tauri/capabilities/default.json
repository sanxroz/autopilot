{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "opener:default",
    "dialog:default",
    "dialog:allow-open",
    "shell:default",
    "shell:allow-spawn",
    "shell:allow-stdin-write",
    "store:default",
    "core:window:allow-start-dragging",
    "core:window:allow-close",
    "core:window:allow-minimize"
  ]
}
